grepl(.$ALT,pattern='-') | grepl(.$ALT,pattern='no_data') | is.na(.$ALT) |
is.na(.$COORDINATE)) %>% nrow(.),
valid_snp = nrow(.)-non_valid_snp,
total_snp = nrow(.))
